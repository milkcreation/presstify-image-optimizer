{
  "name": "presstify-components/image-optimizer",
  "description": "Optimisation d'images",
  "type": "presstify-component",
  "homepage": "https://github.com/milkcreation/presstify-image-optimizer.git",
  "time": "2021-09-13",
  "license": "proprietary",
  "authors": [
    {
      "name": "Jordy Manner",
      "email": "jordy.manner@milkcreation.fr"
    }
  ],
  "require": {
    "php": "^7.0",
    "ext-fileinfo": "*",
    "composer/installers": "~1.0",
    "spatie/image": "^1.5"
